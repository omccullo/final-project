<%@company_name=params.fetch("company")%>
<%@position_type=params.fetch("position_type")%>
<%@company=CompanyRole.where({:name=>@company_name, :full_part_time=>@position_type}).at(0)%>
<%@company_id=@company.id.to_i%>
<%@role=params.fetch("role")%>
<%@interview_round=params.fetch("round")%>

<hr>

<div>
  <form action="/new_questions"> <!-- figure out if this route already exists-->
    <%question_number=0%>
    <%InterviewQuestionEntry.where({:company_roles_id=>@company_id}).each do |a_question|%>
    <%question_id=a_question.question_id%>
    <%@question=Question.where({:id=>question_id}).at(0)%>
    <div class="form-check">
      <input class="form-check-input" type="checkbox" value="" id="question<%question_number%>">
      <label class="form-check-label" for="question<%question_number%>">
         <%=@question.text%>
      </label>
</div>
    <%question_number=question_number+1%>
    <%end%>
</div>

<button class="btn btn-primary">Next</button>
  
</form>
